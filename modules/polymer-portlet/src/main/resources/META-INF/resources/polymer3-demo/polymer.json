
{
  "root": ".",
  "entrypoint": "index.html",
  "shell": "src/app/polymer3-demo-app.js",
  "fragments": [
    "src/routes/app-routing.js",
    "src/views/404.js",
    "src/views/employee-list.js",
    "src/views/employee-new.js"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "node_modules/@babel/polyfill/dist/**",
    "node_modules/mock-http-request/**",
    "employees.json"
  ],
  "builds": [
    {
      "name": "esm-bundled",
      "basePath": "/o/polymer-portlet/polymer3-demo/build/esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "esm-unbundled",
      "basePath": "/o/polymer-portlet/polymer3-demo/build/esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "basePath": "/o/polymer-portlet/polymer3-demo/build/es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
        "name": "es5-bundled",
        "basePath": "/o/polymer-portlet/polymer3-demo/build/es5-bundled",
        "js": {
            "minify": true,
            "compile": true,
            "transformModulesToAmd": true
        },
        "css": {
            "minify": true
        },
        "html": {
            "minify": true
        },
        "bundle": true,
        "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
